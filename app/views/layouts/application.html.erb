<!DOCTYPE html>
<html>
  <head>
    <title>CommuteTracker</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <ul>
      <% if current_user %>
       <h1>Hello</h1>
       <% else %>
      <li><%= link_to "Sign in With Google", login_path %></li>
      <% end %>
    </ul>
    <%= yield %>
  </body>
</html>
